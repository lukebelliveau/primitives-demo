{
  "name": "primitives-demo",
  "version": "1.0.0",
  "description": "demo'ing react-primitives & styled-components in web, native, vr, and sketch",
  "scripts": {
    "build": "skpm build",
    "watch": "skpm build --watch",
    "render": "skpm build --watch --run",
    "render:once": "skpm build --run",
    "link-plugin": "skpm link",
    "postinstall": "cd ./web && yarn install && cd ../native && yarn install",
    "sketch": "cd ./sketch && yarn render",
    "web": "cp ./example.js ./web/src/example.js && cd ./web && yarn start",
    "native": "cp ./example.js ./native/example.js && cd ./native && yarn start"
  },
  "author": "Luke Belliveau <luke.belliveau@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "skpm": "^0.9.0"
  },
  "dependencies": {
    "chroma-js": "^1.2.2",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-primitives": "^0.4.2",
    "react-sketchapp": "^0.11.0",
    "react-test-renderer": "^15.4.2",
    "styled-components": "^2.1.0"
  },
  "repository": "github.com/lukebelliveau/primitives-demo"
}
